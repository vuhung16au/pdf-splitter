{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom", "dom.iterable"],
    "types": ["cypress", "node"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
